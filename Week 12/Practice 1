#include <iostream>
#include <string>
using namespace std;

int main() {
	cout << "Type any strings you want to convert.\n";
	string s;
	while (getline(cin, s)) {
		int len = s.length();
		int tmp;
		for (int i = 0; i < len; i++) {
			tmp = static_cast<int>(s[i]);
			cout << tmp << " ";
		}
		cout << "\n";
	}
	return 0;
}

