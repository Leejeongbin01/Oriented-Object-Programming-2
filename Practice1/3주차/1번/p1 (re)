#include <iostream>
using namespace std;

class p1 {
private:
	int* ptr=NULL;
	int a, b, c;
public:
	p1()
		:a(1), b(2), c(3) {
		ptr = new int[3];
		cout << "default\n";
	}
	p1(int x, int y, int z)
		:a(x), b(y), c(z) {
		cout << "para\n";
	}
	p1(const p1& org) {
		a = org.a;
		b = org.b;
		c = org.c;
		cout << "copy\n";
	}
	~p1() {
		if (ptr != NULL) {
			delete[]ptr;
			cout << "dlete";
		}
	}
	void print() {
		cout << a << " " << b << " " << c<<"\n";
	}
};

int main() {

	p1 d;
	p1 p(1, 2, 3);
	p1 c = p;
	c.print();
	return 0;
}
